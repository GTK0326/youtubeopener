{
  "name": "Youtube Opener",
  "version": "1.1",
  "content_scripts":[
    {
      "matches":["https://www.google.tk/?gl=tk*"],
      "js":["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent":false
  },
  "browser_action":{
    "name":"trigger"
    },
  "permissions": ["tabs","http://*/*","https://*/*"],
  "manifest_version": 2
}
